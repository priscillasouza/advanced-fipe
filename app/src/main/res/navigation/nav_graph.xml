<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/onboardingFragment">

    <activity
        android:id="@+id/main_activity"
        android:name="com.advancedfipe.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <fragment
        android:id="@+id/onboardingFragment"
        android:name="com.advancedfipe.presentation.fragments.OnboardingFragment"
        android:label="fragment_init_onboarding"
        tools:layout="@layout/fragment_onboarding">
        <action
            android:id="@+id/action_initOnboarding_self"
            app:destination="@id/onboardingFragment"
            app:enterAnim="@anim/nav_default_enter_anim" />
        <argument
            android:name="pages"
            app:argType="integer"
            android:defaultValue="1"/>
        <action
            android:id="@+id/action_onboardingFragment_to_optionsConsultFragment"
            app:destination="@id/optionsConsultFragment" />
    </fragment>
    <fragment
        android:id="@+id/optionsConsultFragment"
        android:name="com.advancedfipe.presentation.fragments.OptionsConsultFragment"
        android:label="optionsConsultFragment" />
</navigation>